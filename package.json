{
    "title": "Brutus Wear",
    "name": "brutus",
    "version": "0.1",
    "scripts": {
        "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
        "build:assets": "node scripts/build-assets.js",
        "build:pug": "node scripts/build-pug.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "start:debug": "npm run build && node scripts/start-debug.js"
    },
    "description": "An HTML landing page template built with Bootstrap",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "homepage": "https://myusername.github.io/brutus",
    "dependencies": {
        "bootstrap": "5.1.3",
        "gh-pages": "^3.2.3"
    },
    "devDependencies": {
        "autoprefixer": "10.3.7",
        "browser-sync": "2.27.5",
        "chokidar": "3.5.2",
        "concurrently": "6.3.0",
        "postcss": "8.3.9",
        "prettier": "2.4.1",
        "pug": "3.0.2",
        "sass": "1.43.2",
        "shelljs": "0.8.4",
        "upath": "2.0.1"
    }
}
